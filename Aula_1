or * self._sigmoid_derivative(self.hidden_output)\n","        # Ajusta os pesos e viés por meio de gradient descent\n","        self.weights_hidden_output += self.hidden_output.T.dot(output_delta) * self.learning_rate\n","        self.bias_output += np.sum(output_delta, axis=0) * self.learning_rate\n","        self.weights_input_hidden += X.T.dot(hidden_delta) * self.learning_rate\n","        self.bias_hidden += np.sum(hidden_delta, axis=0) * self.learning_rate\n","\n","    def fit(self, X, y):\n","        for _ in range(self.epochs):\n","            self.forward(X)\n","            self.backward(X, y)\n","\n","    def predict(self, X):\n","        return self.forward(X)"],"metadata":{"id":"P1NQWbntOdtL","executionInfo":{"status":"ok","timestamp":1758307137377,"user_tz":180,"elapsed":9,"user":{"displayName":"Felipe Marques","userId":"01646948946098734844"}}},"execution_count":33,"outputs":[]},{"cell_type":"code","source":["X, y = make_moons(n_samples=200, noise=0.1, random_state=42)\n","y = np.expand_dims(y, axis=1)\n","\n","model = MultilayerPerceptron(input_size=X.shape[1], hidden_size=3, epochs=10_000)\n","model.fit(X, y)\n","y_pred = model.predict(X)\n","\n","plot_decision_boundary(model, X, y[:,0])\n","plt.show()"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":449},"id":"4aRljCVrOf-R","executionInfo":{"status":"ok","timestamp":1758307148125,"user_tz":180,"elapsed":928,"user":{"displayName":"Felipe Marques","userId":"01646948946098734844"}},"outputId":"a41daa1b-e6ed-451b-b521-6510e9c1597b"},"execution_count":35,"outputs":[{"output_type":"display_data","data":{"text/plain":["<Figure size 640x480 with 1 Axes>"],"image/png":"\n"},"metadata":{}}]}]}